.swiper-container {
    position: relative;
}
/* 首页偏离 */
.cu-bar .content {
  text-align: left;
  margin-left: 25rpx;
}
/* swiper轮播控制点 */
.swiper-container .dots {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 35rpx;
    display: flex;
    justify-content: center;
}

.swiper-container .dots .dot {
    margin: 0 8rpx;
    width: 14rpx;
    height: 14rpx;
    background: #fff;
    border-radius: 8rpx;
    transition: all .6s;
}

.swiper-container .dots .dot.active {
    width: 24rpx;
    background: #f80;
}

